{
  "name": "management-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.13.6",
    "env-cmd": "^8.0.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-input-mask": "^2.0.4",
    "react-loading-skeleton": "^1.1.1",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "typescript": "3.3.3333"
  },
  "scripts": {
    "start": "env-cmd .env.local react-app-rewired start",
    "build": "react-app-rewired build",
    "build:dev": "env-cmd .env.dev yarn build",
    "build:int": "env-cmd .env.integration yarn build",
    "build:production": "env-cmd .env.production yarn build",
    "test": "react-app-rewired test",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "tslint -p tsconfig.json --format stylish",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit-msg && yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "24.0.6",
    "@types/node": "11.9.4",
    "@types/react": "16.8.4",
    "@types/react-dom": "16.8.2",
    "@types/react-input-mask": "^2.0.1",
    "@types/react-redux": "^7.0.1",
    "@types/redux-logger": "^3.0.7",
    "@babel/core": "^7.3.4",
    "@commitlint/config-conventional": "^7.5.0",
    "antd-scss-theme-plugin": "^1.0.7",
    "babel-jest": "23.6.0",
    "babel-plugin-import": "^1.11.0",
    "commitlint": "^7.5.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "husky": "^1.3.1",
    "node-sass": "^4.11.0",
    "jest": "23.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "prettier": "^1.16.4",
    "react-app-rewired": "^2.1.1",
    "react-scripts": "2.1.5",
    "redux-logger": "^3.0.6",
    "tslint": "^5.13.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typesafe-actions": "^3.2.1"
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
}